{
  "name": "daisyui-ntsd",
  "version": "1.0.9",
  "description": "A DaisyUI theme from @ntsd",
  "author": "Jirawat Boonkumnerd <contact@ntsd.dev> (https://ntsd.dev)",
  "main": "./dist/cjs/theme.js",
  "types": "./dist/types/theme.d.ts",
  "module": "./dist/esm/theme.js",
  "files": [
    "./dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/theme.d.ts",
      "require": "./dist/cjs/theme.js",
      "import": "./dist/esm/theme.js",
      "default": "./dist/esm/theme.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntsd/daisyui-ntsd.git"
  },
  "keywords": [
    "daisyui",
    "theme"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ntsd/daisyui-ntsd/issues"
  },
  "homepage": "https://github.com/ntsd/daisyui-ntsd#readme",
  "dependencies": {
    "chroma-js": "^2.4.2"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.4.1"
  }
}